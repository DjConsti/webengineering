<?xml version="1.0" encoding="UTF-8"?>
<process
    name="BPEL_SOA"
    targetNamespace="http://enterprise.netbeans.org/bpel/BPEL_SOA/BPEL_SOA"
    xmlns:tns="http://enterprise.netbeans.org/bpel/BPEL_SOA/BPEL_SOA"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://soa/xsd">
    <import namespace="http://enterprise.netbeans.org/bpel/erstelleEintragWrapper" location="erstelleEintragWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://soa/" location="http://localhost:8146/axis2/services/erstelleEintrag?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="Erstellen" xmlns:tns="http://enterprise.netbeans.org/bpel/erstelleEintragWrapper" partnerLinkType="tns:erstelleEintragLinkType" myRole="erstelleEintragRole"/>
    </partnerLinks>
    <variables>
        <variable name="ErstellEintragOut" xmlns:axis2="http://soa/" messageType="axis2:erstellEintragResponse"/>
        <variable name="ErstellEintragIn" xmlns:axis2="http://soa/" messageType="axis2:erstellEintragRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="Erstellen" operation="erstellEintrag" xmlns:axis2="http://soa/" portType="axis2:erstelleEintragPortType" variable="ErstellEintragIn"/>
        <assign name="Assign1">
            <copy>
                <from>true()</from>
                <to>$ErstellEintragOut.parameters/ns0:return</to>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="Erstellen" operation="erstellEintrag" xmlns:axis2="http://soa/" portType="axis2:erstelleEintragPortType" variable="ErstellEintragOut"/>
    </sequence>
</process>

